
@{
    ViewBag.Title = "ShowClaims";
}

<h2>@ViewBag.Title</h2>
<h2>@ViewBag.Message</h2>

@model IEnumerable<System.Security.Claims.Claim>

<p>
    <a href="https://localhost:44392/Account/SignIn" target="_blank"> https://localhost:44392/Account/SignIn </a> it's not needed to sign in any more

</p>
<dl>
    @foreach (var claim in Model)
    {
        <dt>@claim.Type</dt>
        <dd>@claim.Value</dd>
    }
</dl>



